#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int valorA;
	int valorB;
	int suma;

	printf("Ingrese el primer numero: ");
	scanf("%d",&valorA);
	printf("Ingrese el segundo numero: ");
	scanf("%d",&valorB);
	suma = valorA + valorB;

	printf("La suma es: %d",suma);

	return 0;
}
